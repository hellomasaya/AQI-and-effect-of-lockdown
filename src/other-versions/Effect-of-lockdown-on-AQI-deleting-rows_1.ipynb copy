   "source": [
    "# 3. Cities and Pollution\n",
    "\n",
    "## a. Most Polluted cities"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 20,
   "metadata": {},
   "outputs": [],
   "source": [
    "def max_polluted_city(pollutant, data):\n",
    "    x1 = data[[pollutant,'City']].groupby([\"City\"]).mean().sort_values(by=pollutant,ascending=False).reset_index()\n",
    "    x1[pollutant] = round(x1[pollutant],2)\n",
    "    return x1[:10].style.background_gradient(cmap='coolwarm')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 21,
   "metadata": {},
   "outputs": [],
   "source": [
    "pm = max_polluted_city('Particulate_Matter', updatedCityData)\n",
    "no2 = max_polluted_city('NO2', updatedCityData)\n",
    "so2 = max_polluted_city('SO2', updatedCityData)\n",
    "co = max_polluted_city('CO', updatedCityData)\n",
    "btx = max_polluted_city('BTX', updatedCityData)\n",
    "o3 = max_polluted_city('O3', updatedCityData)"
   ]
  },
