    "def getCities(data):\n",
    "    cities = data['City'].value_counts().to_frame()\n",
    "    print('------------------------------------------------------------------------------')\n",
    "    print('CITIES COVERED IN THE DATA:')\n",
    "    print('------------------------------------------------------------------------------')\n",
    "    print('Total number of cities in the dataset :', len(cities))\n",
    "    cities = cities.sort_index().index\n",
    "    for i in cities:\n",
    "        print(i)\n",
    "#     print('------------------------------------------------------------------------------')"
   ]
