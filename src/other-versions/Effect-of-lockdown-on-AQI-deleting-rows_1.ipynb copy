    "### Changing data structure to City-wise Monthly data"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 33,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "Index(['City', 'Date', 'AQI', 'AQI_Bucket'], dtype='object')"
      ]
     },
     "execution_count": 33,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "#using cities for Comparision\n",
    "cities=pd.unique(updatedCityData['City'])\n",
    "column1= cities+'_AQI'\n",
    "column2=cities+'_AQI_Bucket'\n",
    "columns=[*column1,*column2]\n",
    "updatedCityData.columns\n",
    "# cities"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 34,
   "metadata": {},
   "outputs": [],
   "source": [
    "#Converting to Monthly data for easier usage\n",
    "final_data= pd.DataFrame(index=np.arange('2015-01-01','2020-07-02',dtype='datetime64[D]'), columns=column1)\n",
    "for city,i in zip(cities, final_data.columns):\n",
    "    n = len(np.array(updatedCityData[updatedCityData['City'] == city]['AQI']))\n",
    "    final_data[i][-n:] = np.array(updatedCityData[updatedCityData['City']==city]['AQI'])\n",
    "\n",
    "final_data=final_data.astype('float64')\n",
    "final_data=final_data.resample(rule='MS').mean()"
   ]
