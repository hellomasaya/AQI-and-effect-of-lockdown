    "# 2. Checking for Missing Values in the data"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [],
   "source": [
    "def getMissingValues(data):\n",
    "    missing_val = data.isnull().sum()\n",
    "    missing_val_percentage = 100 * data.isnull().sum() / len(data)\n",
    "    missin_values_array = pd.concat([missing_val, missing_val_percentage], axis=1)\n",
    "    missin_values_array = missin_values_array.rename(columns = \n",
    "                                                     {0 : 'Missing Values', 1 : '% of Total Values'})\n",
    "    missin_values_array = missin_values_array[\n",
    "        missin_values_array.iloc[:,1] != 0].sort_values('% of Total Values', ascending=False).round(1)\n",
    "    print('------------------------------------------------------------------------------')\n",
    "    print('MISSING VALUES IN DATA:\\n------------------------------------------------------------------------------\\n', missin_values_array, '\\n')\n",
    "    return missin_values_array"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {},
   "outputs": [],
   "source": [
    "def mergeColumns(data):\n",
    "    data['Date'] = pd.to_datetime(data['Date'])\n",
    "    data['BTX'] = data['Benzene'] + data['Toluene'] + data['Xylene']\n",
    "    data.drop(['Benzene','Toluene','Xylene'], axis=1)\n",
    "    data['Particulate_Matter'] = data['PM2.5'] + data['PM10']\n",
    "    return data"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "metadata": {},
   "outputs": [],
   "source": [
    "def subsetColumns(data):\n",
    "    pollutants = ['Particulate_Matter', 'NO2', 'CO','SO2', 'O3', 'BTX']\n",
    "    columns =  ['Date', 'City', 'AQI', 'AQI_Bucket'] + pollutants\n",
    "    data = data[columns]\n",
    "    return data, pollutants"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {},
   "outputs": [],
   "source": [
    "def handleMissingValues(data):\n",
    "    missing_values = getMissingValues(data)\n",
    "    updatedCityData = mergeColumns(data)\n",
    "    updatedCityData, pollutants = subsetColumns(updatedCityData)\n",
    "    return updatedCityData, pollutants"
   ]
