    "## b. Visualising the amount of pollutants in air over the years and months"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "metadata": {},
   "outputs": [],
   "source": [
    "def trend_plot(updatedCityData, value):\n",
    "    data = updatedCityData.copy()\n",
    "    data['Year'] = [d.year for d in data.Date]\n",
    "    data['Month'] = [d.strftime('%b') for d in data.Date]\n",
    "    years = data['Year'].unique()\n",
    "    fig, axes = plt.subplots(1, 2, figsize=(12,3), dpi= 80)\n",
    "    sns.boxplot(x='Year', y=value, data=data, ax=axes[0])\n",
    "    sns.pointplot(x='Month', y=value, data=data.loc[~data.Year.isin([2015, 2020]), :])\n",
    "    \n",
    "    axes[0].set_title('Year-wise Plot i.e. the trend', fontsize=18); \n",
    "    axes[1].set_title('Month-wise Plot i.e. the seasonality', fontsize=18)\n",
    "    plt.show()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "metadata": {
    "scrolled": true
   },
   "outputs": [
