    "# 3. Visualising the time series data\n",
    "\n",
    "## a. Visualising the amount of pollutants in air over the years"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "metadata": {},
   "outputs": [],
   "source": [
    "def visualisePollutants(udata, columns):\n",
    "    data = udata.copy()\n",
    "    data.set_index('Date',inplace=True)\n",
    "    axes = data[columns].plot(marker='.', linestyle='None', figsize=(15, 15), subplots=True)\n",
    "    for ax in axes:\n",
    "        ax.set_xlabel('Years')\n",
    "        ax.set_ylabel('ug/m3')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
